{
	"manifest_version": 2,
	"name": "PokeBin-Extension",
	"version": "1.0",
	"description": "Enable imports / exports from PokeBin",
	"icons": {
		"48": "icons/spheal.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://play.pokemonshowdown.com/teambuilder",
				"*://pokebin.com/*/json"
			],
			"js": ["main.js"]
		}
	]
}
